{
  "name": "toratech-popout-widget",
  "version": "1.0.0",
  "description": "ToRaTech Pop-out Chat Widget - A lightweight, embeddable chat widget",
  "main": "dist/widget.min.js",
  "scripts": {
    "build": "webpack --config webpack.widget.config.js",
    "build:dev": "webpack --config webpack.widget.config.js --mode development",
    "build:watch": "webpack --config webpack.widget.config.js --watch",
    "dev": "webpack serve --config webpack.widget.config.js --mode development",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "chat",
    "widget",
    "popout",
    "customer-support",
    "toratech"
  ],
  "author": "ToRaTech",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/toratech/popout-chat-widget.git"
  },
  "homepage": "https://github.com/toratech/popout-chat-widget#readme",
  "dependencies": {
    "@babel/runtime": "^7.27.1"
  }
}
